        <TableView fx:id="userTable" prefHeight="336.0" prefWidth="862.0">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="80.0" text="User ID" />
                <TableColumn fx:id="nameColumn" prefWidth="150.0" text="Name" />
                <TableColumn fx:id="usernameColumn" prefWidth="150.0" text="Username" />
                <TableColumn fx:id="emailColumn" prefWidth="200.0" text="Email" />
                <TableColumn fx:id="passwordColumn" prefWidth="150.0" text="Password" />
