            </columns>
        </TableView>
    </VBox>
     <Button fx:id="deleteButton" layoutX="361.0" layoutY="476.0" prefWidth="126.0" text="Delete Selected User" />
   <Button layoutX="729.0" layoutY="476.0" mnemonicParsing="false" onAction="#onBackbuttonclick" prefHeight="26.0" prefWidth="54.0" text="Back" />
